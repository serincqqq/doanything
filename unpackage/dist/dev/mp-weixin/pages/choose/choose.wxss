	right: 0;
	width: 100%;
	height: 100%;
	background: rgba(0, 0, 0, 0.1);
}
.flip-container.data-v-c102b434 {
	z-index: 999;
	-webkit-perspective: 1000;
	        perspective: 1000;
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
}
.flip-container.data-v-c102b434,
.front.data-v-c102b434,
.back.data-v-c102b434,
.face.data-v-c102b434 {
	width: 450rpx;
	height: 650rpx;
}
.face.data-v-c102b434 {
	position: absolute;
	left: 50%;
	top: 50%;
	-webkit-transform: translate(-50%, -50%);
	        transform: translate(-50%, -50%);
}
/* flip speed goes here */
.flipper.data-v-c102b434 {
	transition: 0.6s;
	-webkit-transform-style: preserve-3d;
	        transform-style: preserve-3d;
	position: relative;
}
/* hide back of pane during swap */
.front.data-v-c102b434,
.back.data-v-c102b434 {
	-webkit-backface-visibility: hidden;
	        backface-visibility: hidden;
	position: absolute;
	top: 0;
	left: 0;
}
.front.data-v-c102b434 {
	z-index: 2;
}
.flip.data-v-c102b434 {
	-webkit-transform: rotateY(180deg);
	        transform: rotateY(180deg);
}

